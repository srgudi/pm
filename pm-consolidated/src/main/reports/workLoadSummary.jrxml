	<queryString>
		<![CDATA[select distinct staff1.name as staff, party.`name` as PName,task.due_date,task.id,
  party.`party_id`, task.`filing_type`,  partner1.`name` as partner , task.`firm_id`,
  client.name as CName,  practice.`id` as PID,  practice.`name` as PCName,task.filing_title,(select type from person_type where type = $P{InCharge}) as Role
  from `task` as task, `party` as party,
       `party` as partner1, `party` as client,
       `party` as staff1,
       `tax_application_practice_type` as practice
