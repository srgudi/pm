<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/</from>
		<to>/Chiguru/welcome</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/$1</to>
	</rule>
	<outbound-rule>
		<from>/Chiguru/**</from>
		<to>/$1</to>
	</outbound-rule>	
</urlrewrite>
